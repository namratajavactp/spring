package periop.springdi;

public class Employee {
	
	String firstname;
	double sal;
	MyDate d;
	
	
	public Employee() {}
	
	public Employee(String firstname,double sal,MyDate d)
	{
		this.firstname=firstname;
		this.sal=sal;
		this.d=d;
		
		
	}
	
	
	public String toString()
	{
		return "firstname="+firstname+",sal="+sal+"d="+d.toString();
	}

	public String getFirstname() {
		return firstname;
	}

	public void setFirstname(String firstname) {
		this.firstname = firstname;
	}

	public double getSal() {
		return sal;
	}

	public void setSal(double sal) {
		this.sal = sal;
	}

	public MyDate getD() {
		return d;
	}

	public void setD(MyDate d) {
		this.d = d;
	}

}
